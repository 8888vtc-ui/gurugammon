# GammonGuru Environment Configuration
# Mode "Zéro Erreur" - Development

NODE_ENV=development
PORT=3000

# Database (PostgreSQL)
DATABASE_URL=postgresql://localhost:5432/gammonguru_dev

# API Keys (à remplir plus tard)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Security
JWT_SECRET=your_jwt_secret_here_change_in_production

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=debug

# GNUBG Configuration
GNUBG_PATH=/usr/local/bin/gnubg
GNUBG_TIMEOUT=30000

# AI Configuration
AI_MAX_TOKENS=2000
AI_TEMPERATURE=0.7

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100
