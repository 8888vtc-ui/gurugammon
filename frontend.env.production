# Priorité production - Configuration pour demain matin
# Frontend Netlify + Backend Railway

# Mode
NODE_ENV=production
VITE_MODE=production

# API Backend Railway (priorité 1)
VITE_API_BASE_URL=https://gammon-guru-backend.railway.app

# Supabase (priorité 1)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# WebSocket (priorité 2)
VITE_WS_URL=wss://gammon-guru-backend.railway.app

# Feature flags pour demain matin
VITE_ENABLE_ANALYSIS=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_MULTIPLAYER=false
VITE_ENABLE_TOURNAMENTS=false

# Performance
VITE_CACHE_DURATION=300000
VITE_REQUEST_TIMEOUT=15000

# Monitoring
VITE_ENABLE_ERROR_REPORTING=true
VITE_SENTRY_DSN=your-sentry-dsn

# Debug (désactiver en production)
VITE_DEBUG=false
